include _mixins
doctype html
html
  head
    include _head

  body(class=bodyClass)
    //BEGIN out
    .out(class=outClass)
      block header
        include ../partials/_header
      block content

    //END out

    //LOAD SCRIPTS
    block scripts
      script(type="text/javascript" src="js/app.js")
      script.
        function initMap() {
        var uluru = {lat: 42.9228996, lng: 47.5211767};
        var map = new google.maps.Map(document.getElementById('map'), {
        zoom: 17,
        center: {lat: 42.92279604, lng: 47.5245765}
        });
        var marker = new google.maps.Marker({
        position: uluru,
        map: map,
        icon: 'img/mark.png'
        });
        }
      script(src='https://maps.googleapis.com/maps/api/js?key=AIzaSyAKcPNdPfc3cYlEYjOqA4kLI_ztCIlSPqc&callback=initMap', async='', defer='')

